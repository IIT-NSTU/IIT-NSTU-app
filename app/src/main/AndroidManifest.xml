<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.whatEver.iitnstu">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.whatEver.iitnstu.StudentAdapter"></activity>
        <activity android:name="com.whatEver.iitnstu.TeachersInfo" />
        <activity android:name="com.whatEver.iitnstu.TeacherCard" />
        <activity android:name="com.whatEver.iitnstu.CourseCoordinator" />
        <activity android:name="com.whatEver.iitnstu.CourseCoCard" />
        <activity android:name="com.whatEver.iitnstu.Developers" />
        <activity android:name="com.whatEver.iitnstu.ThirdBatchInfo" />
        <activity android:name="com.whatEver.iitnstu.Officials" />
        <activity android:name="com.whatEver.iitnstu.OfficialsCard" />
        <activity
            android:name="com.whatEver.iitnstu.Search"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.whatEver.iitnstu.PdfViewer" />
        <activity android:name="com.whatEver.iitnstu.SyllabusMain" />
        <activity android:name="com.whatEver.iitnstu.StudentCard" />
        <activity android:name="com.whatEver.iitnstu.SecondBatchInfo" />
        <activity android:name="com.whatEver.iitnstu.DirectorProfile" />
        <activity android:name="com.whatEver.iitnstu.StudentInfoMain" />
        <activity
            android:name="com.whatEver.iitnstu.StudentInfoLogin"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name="com.whatEver.iitnstu.AllCourseDetails" />
        <activity android:name="com.whatEver.iitnstu.IntroActivity" />
        <activity android:name="com.whatEver.iitnstu.HomeActivity" />
        <activity android:name="com.whatEver.iitnstu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>